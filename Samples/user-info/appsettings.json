  "Logging": {
    "IncludeScopes": false,
    "LogLevel": {
      "Default": "Debug",
      "System": "Information",
      "Microsoft": "Information"
    }
  },
  "kestrel": {
    "EndPoints": {
      "Http": {
        "Url": "http://*:3010"
      }
    }
  },
  "auth0": {
    "domain": "{DOMAIN}",
    "apiIdentifier": "{API_IDENTIFIER}"
  }
}
